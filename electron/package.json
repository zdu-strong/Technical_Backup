{
  "name": "my-app",
  "version": "0.1.0",
  "scripts": {
    "start": "node bin/pre_load_configuration.js && node bin/start.js",
    "pack": "node bin/pre_load_configuration.js && node bin/pack.js",
    "make": "node bin/pre_load_configuration.js && node bin/make.js"
  },
  "main": "./dist/index.js",
  "private": true,
  "homepage": ".",
  "build": {
    "extends": null,
    "files": [
      "dist",
      "public",
      "build"
    ],
    "directories": {
      "output": "output"
    },
    "appId": "com.electron.electron",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "deleteAppDataOnUninstall": true,
      "guid": "b5a96e2b-5f4f-4a67-b77a-2c9d7c947242",
      "runAfterFinish": true,
      "allowToChangeInstallationDirectory": true
    },
    "asar": {
      "smartUnpack": true
    },
    "asarUnpack": [
      "public",
      "build",
      "node_modules/@electron/remote",
      "node_modules/node-os-utils"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "import/no-anonymous-default-export": [
        "off",
        {
          "allowArrowFunction": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@babylonjs/core": "7.45.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@fontsource/roboto": "5.1.1",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@mui/lab": "6.0.0-beta.24",
    "@mui/material": "6.4.1",
    "@mui/x-charts": "7.24.1",
    "@mui/x-data-grid": "7.24.1",
    "@mui/x-date-pickers": "7.24.1",
    "@nestjs/cli": "11.0.2",
    "@types/node": "22.12.0",
    "@types/node-os-utils": "1.3.4",
    "@types/node-rsa": "1.1.4",
    "@types/qs": "6.9.18",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/wait-on": "5.3.4",
    "autoprefixer": "10.4.20",
    "axios": "1.7.9",
    "date-fns": "4.1.0",
    "dexie": "4.0.11",
    "electron": "34.0.1",
    "electron-builder": "25.1.8",
    "eslint": "8.57.0",
    "execa": "5.1.1",
    "get-port": "5.1.1",
    "is-mobile": "5.0.0",
    "jsencrypt": "3.3.2",
    "mathjs": "14.1.0",
    "mobx-react-use-autorun": "4.0.52",
    "node-polyfill-webpack-plugin": "4.0.0",
    "node-rsa": "1.1.1",
    "postcss": "8.4.49",
    "qs": "6.14.0",
    "react": "18.3.1",
    "react-app-rewired": "2.2.1",
    "react-dom": "18.3.1",
    "react-git-info": "2.0.1",
    "react-intl": "6.8.7",
    "react-router-dom": "6.28.0",
    "react-scripts": "5.0.1",
    "rxjs-exhaustmap-with-trailing": "2.1.1",
    "source-map-support": "0.5.21",
    "tailwindcss": "3.4.17",
    "ts-loader": "9.5.2",
    "tsconfig-paths": "4.2.0",
    "typedjson": "1.8.0",
    "typescript": "4.9.5",
    "typestyle": "2.4.0",
    "wait-on": "8.0.2",
    "web-vitals": "4.2.4",
    "webworker-promise": "0.5.1",
    "windows-locale": "1.1.3"
  },
  "dependencies": {
    "@electron/remote": "2.1.2",
    "linq": "3.2.4",
    "node-os-utils": "1.3.7",
    "rxjs": "7.8.1",
    "stream-to-string": "1.2.1",
    "tree-kill": "1.2.2",
    "uuid": "11.0.5"
  }
}
